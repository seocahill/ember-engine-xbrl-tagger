<ul class="list-group">
  {{#each currentCell.cell.dimensions as |dimension|}} 
    <li class="list-group-item">
      <div class="row">
        <div class="col-10">
          <div class="row">
            <div class="col">
               {{dimension.hypercubeDimension}}
            </div>
            <div class="col">
              {{dimension.domainMember}}
            </div>
          </div>
        </div>
        <div class="col text-right"><a href="#" {{action "delete" dimension}}><i class="fa fa-trash fa-2x"></i></a></div>
      </div>
    </li>
     {{link-to "Add custom dimensions" 'dimension-nodes' class="list-group-item"}}
  {{else if (and currentCell.cell.settings.name currentCell.cell.period)}}
    {{link-to "Pick custom dimensions" 'dimension-nodes' class="list-group-item"}}
  {{else}}
    <li class="list-group-item">Not tagged.</li>
  {{/each}}
</ul>